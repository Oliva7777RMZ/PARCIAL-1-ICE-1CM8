#include <stdio.h>
#include <math.h>
int main(){
    //compilar celsius a fahrenheit
    float celsius,fahrenheit;
    //pedir datos
    printf("Ingresa grados celsius");
    scanf("%f",&celsius);

    //formula
    fahrenheit = celsius * 1.8 + 32;
  
    //resultado
    printf("los grados celsius son %f\nlos grados fahrenheit son %f\n",celsius,fahrenheit);
    
    return 0;
}
